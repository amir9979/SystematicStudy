import org.eclipse.epf.library.edit.uma.impl.ScopeImpl;
import org.eclipse.epf.library.edit.util.LibraryEditUtil;
public Scope newProcessScope(String name) {
Scope scope = new ScopeImpl();

if (name != null) {
scope.setName(name);
}

return scope;


private CancelUtil() {}

public static boolean isCancelled(Object o)
{
boolean isCancelled = false;

if (o != null &amp;&amp; o instanceof Cancellable)
return isCancelled;
}

public static void cancel(Object o)
{
if (o != null &amp;&amp; o instanceof Cancellable)
{
((Cancellable)o).cancel();
}
}
}


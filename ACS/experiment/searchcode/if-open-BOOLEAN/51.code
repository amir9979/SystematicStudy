public ElectricWindow () {
open = false;
System.out.println(&quot;Window is closed&quot;);
}

public boolean isOpen () {
System.out.println(&quot;Window is now open&quot;);
}
}

public void closeWindow() {
if (isOpen()) {
open = false;
System.out.println(&quot;Window is now closed&quot;);
}
}

private boolean open;
}


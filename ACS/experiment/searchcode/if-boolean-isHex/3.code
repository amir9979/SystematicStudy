package progetto.util;

public class Utils {
public static boolean isHex(String s) {
char c;
for(int i=0;i<s.length();i++) {
c=s.charAt(i);
if(!(c>=&#39;0&#39; &amp;&amp; c<=&#39;9&#39; || c>=&#39;a&#39; &amp;&amp; c<=&#39;f&#39; || c>=&#39;A&#39; &amp;&amp; c<=&#39;F&#39;))
return false;
}
return true;


}
}


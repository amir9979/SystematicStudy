private int[] synthesize(int divisorNum, int[] coefficients) {
int[] newNums = new int[coefficients.length];

newNums[0] = coefficients[0];
public boolean isPolynomialFactor(int divisorNum, int[] coefficients) {
int[] newNums = synthesize(divisorNum, coefficients);
if (newNums[newNums.length - 1] == 0) {
return true;
} else {
return false;
}
}
}


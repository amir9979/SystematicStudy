return true;
}
}
}
return false;
}
public boolean isEarlierThan(Calendar calendar){
if (get(Calendar.YEAR) < calendar.get(Calendar.YEAR)){
return false;
}
public boolean isLaterThan(Calendar calendar){
if (!isEarlierThan(calendar) &amp;&amp; !equals(calendar)){
return true;
}
else {
return false;
}
}
}


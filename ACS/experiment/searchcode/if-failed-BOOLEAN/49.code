if(!exp) perr(&quot;Assertion failed&quot;);
}

public final static void is_false(boolean exp){
if(exp) perr(&quot;Assertion failed&quot;);
if(!exp) perr(&quot;Assertion failed: &quot; + msg);
}

public final static void is_false(boolean exp, String msg){
if(exp) perr(&quot;Assertion failed: &quot; + msg);
}

return new TripleDouble(x + t.X(), y + t.Y(), z + t.Z());
}

//don&#39;t need another one for int as it is converted automatically
public TripleDouble subtract(TripleInt t) {
return new TripleDouble(x - t.X(), y - t.Y(), z - t.Z());
}

